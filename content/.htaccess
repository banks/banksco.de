RewriteEngine  on

# Redirect old domain
RewriteEngine On
RewriteCond %{HTTP_HOST} ^blg\.banksdesigns\.co\.uk$ [OR]
RewriteCond %{HTTP_HOST} ^blog\.banksdesigns\.co\.uk$ [OR]
RewriteCond %{HTTP_HOST} ^www\.banksco\.de$
RewriteRule (.*)$ http://banksco.de/$1 [R=301,L]

RewriteRule    ^post/(.*) 	/p/$1.html [L,R=301]
RewriteRule    ^rss$ 		/rss.xml [L,R=301]
RewriteRule    ^archive$ 	/archive.html [L,R=301]

ErrorDocument 400 /error/400.html
ErrorDocument 401 /error/401.html
ErrorDocument 403 /error/403.html
ErrorDocument 404 /error/404.html
ErrorDocument 500 /error/500.html